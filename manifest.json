{
    "manifest_version": 3,
    "name": "Bionic Reading Enhancer",
    "version": "1.0",
    "description": "Enhances webpages with Bionic Reading.",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }
  